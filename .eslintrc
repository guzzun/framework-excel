{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
      },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "linebreak-style": ["error", "windows"]
    }
}